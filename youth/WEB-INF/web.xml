<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Investor Self Service Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>CompanyRegister</servlet-name>
    <servlet-class>com.CompanyRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompanyRegister</servlet-name>
    <url-pattern>/CompanyRegister</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StudentRegister</servlet-name>
    <servlet-class>com.StudentRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentRegister</servlet-name>
    <url-pattern>/StudentRegister</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>Company</servlet-name>
    <servlet-class>com.Company</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Company</servlet-name>
    <url-pattern>/Company</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>Student</servlet-name>
    <servlet-class>com.Student</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Student</servlet-name>
    <url-pattern>/Student</url-pattern>
  </servlet-mapping>

   <servlet>
    <servlet-name>AddJob</servlet-name>
    <servlet-class>com.AddJob</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddJob</servlet-name>
    <url-pattern>/AddJob</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Apply</servlet-name>
    <servlet-class>com.Apply</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Apply</servlet-name>
    <url-pattern>/Apply</url-pattern>
  </servlet-mapping>

</web-app>